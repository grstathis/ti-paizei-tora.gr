    "\n",
    "# Basic Greek -> Latin transliteration suitable for URL slugs\n",
    "GREEK_TO_LATIN = {\n",
    "    # lowercase\n",
    "    \"Œ±\":\"a\",\"Œ¨\":\"a\",\"Œ≤\":\"v\",\"Œ≥\":\"g\",\"Œ¥\":\"d\",\"Œµ\":\"e\",\"Œ≠\":\"e\",\"Œ∂\":\"z\",\"Œ∑\":\"i\",\"ŒÆ\":\"i\",\"Œ∏\":\"th\",\n",
