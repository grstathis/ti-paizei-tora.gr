    "    \"œÄ\":\"p\",\"œÅ\":\"r\",\"œÉ\":\"s\",\"œÇ\":\"s\",\"œÑ\":\"t\",\"œÖ\":\"y\",\"œç\":\"y\",\"œã\":\"y\",\"Œ∞\":\"y\",\"œÜ\":\"f\",\"œá\":\"x\",\n",
    "    \"œà\":\"ps\",\"œâ\":\"o\",\"œé\":\"o\",\n",
